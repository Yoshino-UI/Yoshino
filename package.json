{
  "name": "yoshino",
  "version": "0.1.4",
  "description": " A customizable PC terminal component library based on react which has strong expansibility, it can output corresponding components according to different business scenarios.",
  "main": "lib/index",
  "module": "es6/index",
  "keywords": [
    "react",
    "UI",
    "component",
    "pc"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yoshino-UI/Yoshino"
  },
  "author": "ShanaMaid",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Yoshino-UI/Yoshino/issues"
  },
  "homepage": "https://github.com/Yoshino-UI/Yoshino#readme",
  "jsnext:main": "es/index",
  "typings": "components/index",
  "scripts": {
    "dev": "webpack-dev-server",
    "build": "node scripts/build.js",
    "site": "webpack",
    "new": "node scripts/new-component.js",
    "new:docs": "node scripts/new-docs.js",
    "precommit": "npm run lint",
    "lint:ts": "tslint -p .",
    "lint:less": "stylelint 'components/**/**.less --syntax less'",
    "lint": "npm run lint:less && npm run lint:ts",
    "test": "jest"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/scripts/jest.setup.js"
    ],
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "^.+\\.css?$": "<rootDir>/scripts/jest.setup.js",
      "^.+\\.svg?$": "<rootDir>/scripts/jest.setup.js"
    },
    "testRegex": "(/__tests__/.*\\.test)\\.tsx?$",
    "moduleFileExtensions": [
      "tsx",
      "ts",
      "js",
      "json"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "components/**/*.tsx",
      "components/**/*.ts",
      "!components/**/*.d.ts"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "text-summary"
    ]
  },
  "engines": {
    "node": ">= 6.0.0"
  },
  "dependencies": {
    "@types/classnames": "*",
    "@types/date-fns": "*",
    "@types/prop-types": "*",
    "@types/react": "^16.3.12",
    "@types/react-dom": "^16.0.5",
    "@types/react-transition-group": "^2.0.8",
    "classnames": "*",
    "date-fns": "*",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "postcss": "^6.0.21",
    "prismjs": "^1.14.0",
    "prop-types": "*",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-transition-group": "^2.3.1",
    "tslib": "*",
    "url-loader": "^1.0.1"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/marked": "^0.3.0",
    "@types/prismjs": "^1.9.0",
    "@types/react-router-dom": "^4.2.6",
    "@types/react-syntax-highlighter": "0.0.5",
    "@types/react-test-renderer": "^16.0.1",
    "css-loader": "^0.28.11",
    "cz-conventional-changelog": "^2.1.0",
    "file-loader": "^1.1.11",
    "fork-ts-checker-webpack-plugin": "^0.4.2",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^3.9.3",
    "gulp-less": "^4.0.0",
    "gulp-postcss": "^7.0.1",
    "highlight.js": "^9.12.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "less-plugin-autoprefix": "^1.5.1",
    "marked": "^0.3.19",
    "postcss-cli": "^4.1.1",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-less": "^1.1.5",
    "postcss-less-engine": "^0.6.2",
    "postcss-loader": "^2.1.4",
    "postcss-nested": "^3.0.0",
    "postcss-px2rem": "^0.3.0",
    "postcss-remove-root": "0.0.2",
    "raw-loader": "^0.5.1",
    "react-docgen-typescript": "^1.3.0",
    "react-route": "^1.0.3",
    "react-router-dom": "^4.2.2",
    "react-styleguidist": "^7.0.7",
    "react-syntax-highlighter": "^7.0.2",
    "react-test-renderer": "^16.3.2",
    "style-loader": "^0.19.1",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.2.0",
    "ts-jest": "^22.4.4",
    "ts-loader": "^4.2.0",
    "tslint": "^5.9.1",
    "tslint-react": "^3.5.1",
    "typescript": "^2.8.3",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
